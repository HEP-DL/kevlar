BEGIN_PROLOG

hdf5FileService: {
  FILENAME: "output.h5"
}

rawDigits: {
  module_type: "HDF5RawDigits"
  ProducerLabel: "daq"
  DataSetLabel: "rawdigits"
  ImageHeight: 9600
  ImageWidth: 3600
  NChannels: 3
  ChunkSize: 10
  FillValue: 0
  CompressionLevel: 5
}

wires: {
  module_type: "HDF5Wires"
  ProducerLabel: "caldata"
  DataSetLabel: "rawdigits"
  ImageHeight: 9600
  ImageWidth: 3600
  NChannels: 3
  ChunkSize: 10
  FillValue: 0
  CompressionLevel: 5
}

particleLabels:{
  module_type: "HDF5ParticleLabelVector"
  Labels: ["e-","e+","proton","pi0","pi+","pi-","mu-","mu+","K+"]
  ProducerLabel: "generator"
  DataSetLabel: "type"
  ChunkSize: 10
  FillValue: 0
  CompressionLevel: 5
}

END_PROLOG