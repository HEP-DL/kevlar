#include "default_hdf5.fcl"

process_name: HDF5Converter

services: {
  HDF5File: @local::hdf5_file_service
}

source: {
  module_type: RootInput
  maxEvents:  -1
}

physics: {
  analyzers: {
    tpcimage: @local::tpcimage
    particlelabels: @local::particlelabels
  }
  ana: [tpc_image, particlelabels ]
  end_paths: [ana]
}
  